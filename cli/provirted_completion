        declare -A options_require_value
        subcommands=()
subcommand_alias=()
subcommand_signs=()
options=(["--ns"]="1" ["--dir"]="1" ["--update"]="1" )
options_require_value=(["--ns"]="1" ["--dir"]="1" )
local argument_min_length=2

